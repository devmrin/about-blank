{
  "name": "quick about-blank",
  "version": "0.1",
  "description": "developer console - about:blank - new tab",
  "background": {
    "scripts": ["background.js"],
    "persistant": false
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_title": "open about:blank in new tab",
    "default_icon": {
      "16": "images/ab16.png",
      "32": "images/ab32.png",
      "48": "images/ab48.png",
      "128": "images/ab128.png"
    }
  },
  "icons": {
    "16": "images/ab16.png",
    "32": "images/ab32.png",
    "48": "images/ab48.png",
    "128": "images/ab128.png"
  },
  "manifest_version": 2
}
